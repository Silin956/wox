{
  "compilerOptions": {
    "strict": true,
    "outDir": "./dist",
    "allowJs": true,
    "target": "es5",
    "sourceMap": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "baseUrl": "./",
    "paths": {
      "#/*": ["./*"]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "baseUrl": "types",
  "typeRoots": ["types"],
}